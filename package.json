{
  "name": "triangle-chan",
  "version": "0.3.3",
  "description": "Triangle Fraternity's own waifu Discord bot!",
  "keywords": [
    "bot",
    "discord",
    "typescript"
  ],
  "type": "module",
  "main": "dist/client.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier -w .",
    "format-check": "prettier -c .",
    "lint": "eslint src",
    "test": "jest",
    "dev": "node --loader ts-node/esm/transpile-only src/index.ts",
    "start": "nodemon --ignore db.json --exec node --loader ts-node/esm/transpile-only src/index.ts",
    "serve": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/evaneliasyoung/triangle-chan.git"
  },
  "author": "Evan Elias Young",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/evaneliasyoung/triangle-chan"
  },
  "homepage": "https://github.com/evaneliasyoung/triangle-chan#readme",
  "private": true,
  "dependencies": {
    "@discordx/importer": "^1.1.2",
    "@discordx/pagination": "^2.0.1",
    "@discordx/utilities": "^4.1.0",
    "axios": "^0.26.1",
    "bignumber.js": "^9.0.2",
    "cheerio": "^1.0.0-rc.10",
    "colors": "1.4.0",
    "discord.js": "^13.5.0",
    "discordx": "^9.1.7",
    "dotenv": "^16.0.0",
    "emoji-regex": "^10.0.0",
    "humanize-duration": "^3.27.1",
    "luxon": "^2.3.1",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.15",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.4.0",
    "typeorm": "^0.3.4",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/humanize-duration": "^3.27.1",
    "@types/luxon": "^2.3.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.12.0",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^2.6.1",
    "ts-jest": "^27.1.4",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
